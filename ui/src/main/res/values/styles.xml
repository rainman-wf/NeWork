<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="OverlyIconOnlyButton" parent="Widget.Material3.Button.IconButton.Filled.Tonal">
        <item name="iconPadding">0dp</item>
        <item name="android:padding">12dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="backgroundTint">?attr/colorSurfaceVariant</item>
        <item name="iconTint">?attr/colorOnSurfaceVariant</item>
        <item name="cornerRadius">12dp</item>
    </style>

    <style name="UneditableInputText" parent="Widget.Material3.TextInputEditText.OutlinedBox">
        <item name="android:inputType">none</item>
        <item name="android:focusable">false</item>
        <item name="android:cursorVisible">false</item>
    </style>

    <style name="FullscreenDialog" parent="MaterialAlertDialog.Material3" >
        <item name="android:insetBottom">0dp</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
        <item name="android:insetTop">0dp</item>
    </style>

</resources>